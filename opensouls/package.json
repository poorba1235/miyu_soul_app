{
  "name": "root",
  "packageManager": "bun@1.2.23",
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "bun-types": "^1.3.4",
    "concurrently": "^9.0.1",
    "eslint": "^8.48.0",
    "inquirer": "^9.3.0",
    "vercel": "^37.3.0",
    "@types/bun": "latest"
  },
  "private": true,
  "scripts": {
    "setup": "bun run scripts/setupRepo.ts",
    "dev:ui": "cd packages/soul-engine-ui && bun dev",
    "dev:cloud": "cd packages/soul-engine-cloud && bun dev",
    "dev:docs": "cd packages/beta-docs && bun dev",
    "start": "concurrently \"bun run dev:cloud\" \"bun run dev:ui\" \"cd packages/beta-docs && PORT=3001 bun dev\""
  },
  "workspaces": [
    "packages/*",
    "souls/**/*"
  ],
  "module": "index.ts",
  "type": "module",
  "peerDependencies": {
    "typescript": "^5"
  }
}
